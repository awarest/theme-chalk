{
  "name": "element-theme-chalk",
  "version": "2.0.11",
  "description": "Element component chalk theme.",
  "main": "index.css",
  "style": "index.css",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "all": "npm run clear; npm run build; npm run build-index",
    "clear": "cd lib; touch _.css; rm *.css",
    "compile": "gulp compile",
    "prettier": "./node_modules/.bin/prettier --write lib/*.css",
    "build": "npm run compile; npm run prettier",
    "make-index": "ls -1 lib/*.css | sed \"s/.*/@import '.\\/&';/\" > index.scss",
    "compile-index": "gulp index",
    "prettier-index": "./node_modules/.bin/prettier --write index.css",
    "build-index": "npm run make-index; npm run compile-index; npm run prettier-index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElementUI/theme-chalk.git"
  },
  "keywords": [
    "element",
    "theme"
  ],
  "author": "yi.shyang@ele.me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ElementUI/theme-chalk/issues"
  },
  "homepage": "https://github.com/ElementUI/theme-chalk#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-sass": "^3.1.0",
    "prettier": "^1.9.2"
  },
  "dependencies": {},
  "browserslist": [
    "Chrome >= 62",
    "Firefox >= 56",
    "Edge >= 16",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "ChromeAndroid >= 61"
  ]
}
